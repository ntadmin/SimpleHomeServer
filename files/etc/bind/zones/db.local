;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	local. homeserver.local. (
			      5		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	local.
@	IN	A	192.168.1.246


homeserver.local.          IN     A       192.168.1.246

